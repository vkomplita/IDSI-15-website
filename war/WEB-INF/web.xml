<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	
	<welcome-file-list>
		<welcome-file>SwissTool.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
    <servlet-name>NewDuck</servlet-name>
    <servlet-class>ch.unige.idsi15.swisstool.NewDuck</servlet-class>
</servlet>

	<servlet-mapping>
    <servlet-name>NewDuck</servlet-name>
    <url-pattern>/new</url-pattern>
</servlet-mapping>

	<servlet>
    <servlet-name>ListDuck</servlet-name>
    <servlet-class>ch.unige.idsi15.swisstool.ListDuck</servlet-class>
</servlet>

	<servlet-mapping>
    <servlet-name>ListDuck</servlet-name>
    <url-pattern>/list</url-pattern>
</servlet-mapping>
	
</web-app>

